<html>
	
	<head>
		<meta charset = utf8>
	</head>

	<body>
	<gif id = "space" src = "space.gif">
		
		<script type="text/javascript/requestAnimationFrame">
			
			function animate(draw, space) {
				var start = performance.now();
				
				for(;;)
				requestAnimationFrame(function animate(time) {
					var timePassed = time - start;

					console.log(time,start)

					draw(timePassed);

					if(timePassed<space) requestAnimationFrame(animate);
				})
			}
		</script>










</body>
</html>